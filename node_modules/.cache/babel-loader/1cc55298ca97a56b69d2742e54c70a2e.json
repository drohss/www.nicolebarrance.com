,GArBA;AA4BA,EAAA,OAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,UAAA;AAAA,QAAA,QAAA,CAAA,WAAA,CAAA,YAAA;AAAA,eAAA,IAAA;AAAA,OAAA,CACA,OAAA,CAAA,EAAA;AAAA,eAAA,KAAA;AAAA;AACA,KAJA;AAKA,IAAA,OALA,mBAKA,MALA,EAKA;AACA,WAAA,QAAA,GAAA,MAAA,CAAA,YAAA,CAAA,CAAA,EAAA,CAAA,CAAA;AACA,WAAA,QAAA,GAAA,IAAA,EAAA,CAAA,MAAA,CAAA,SAAA,CAAA,EAAA,CAAA;AACA,WAAA,QAAA,CAAA,IAAA,CAAA,CAAA;AACA,WAAA,KAAA,GAAA,MAAA,CAAA,SAAA,CAAA,IAAA,CAAA;AACA,MAAA,MAAA,CAAA,SAAA,CAAA,EAAA;AACA,KAXA;AAYA,IAAA,KAZA,iBAYA,MAZA,EAYA;AACA,MAAA,MAAA,CAAA,YAAA,CAAA,MAAA,CAAA,UAAA,EAAA,MAAA,CAAA,WAAA;AACA,MAAA,MAAA,CAAA,IAAA,CAAA,KAAA,MAAA,CAAA,KAAA,cAAA,CAAA;AACA,KAfA;AAgBA,IAAA,IAhBA,gBAgBA,MAhBA,EAgBA;AACA,MAAA,MAAA,CAAA,UAAA,CAAA,MAAA,EADA,CAGA;;AACA,UAAA,YAAA,GAAA,KAAA,sBAAA,CAAA,MAAA,CAAA;;AACA,UAAA,YAAA,EAAA;AACA,aAAA,cAAA,IAAA,KAAA,MAAA,CAAA,MAAA,GAAA,CAAA,GAAA,KAAA,cAAA,GAAA,CAAA,GAAA,KAAA,cAAA,EAAA;AACA,QAAA,MAAA,CAAA,IAAA,CAAA,KAAA,MAAA,CAAA,KAAA,cAAA,CAAA;AACA;;AAEA,WAAA,QAAA,CAAA,GAAA,CAAA,KAAA,QAAA,CAAA,CAAA,EAAA,KAAA,QAAA,CAAA,CAAA,EAAA,KAAA,QAAA,CAAA,CAAA;AAEA,MAAA,MAAA,CAAA,KAAA,CAAA,KAAA,KAAA,EAAA,KAAA,QAAA,CAAA,CAAA,EAAA,KAAA,QAAA,CAAA,CAAA,EAAA,KAAA,cAAA,CAAA,KAAA,EAAA,KAAA,cAAA,CAAA,MAAA;AAEA,KA9BA;AA+BA,IAAA,sBA/BA,kCA+BA,MA/BA,EA+BA;AACA,UAAA,YAAA,GAAA,KAAA,CADA,CAGA;;AACA,UAAA,KAAA,QAAA,CAAA,CAAA,GAAA,CAAA,IAAA,KAAA,QAAA,CAAA,CAAA,GAAA,KAAA,cAAA,CAAA,KAAA,GAAA,MAAA,CAAA,KAAA,EAAA;AACA,aAAA,QAAA,CAAA,CAAA,IAAA,CAAA,CAAA;AACA,QAAA,YAAA,GAAA,IAAA;AACA,OAPA,CASA;;;AACA,UAAA,KAAA,QAAA,CAAA,CAAA,GAAA,CAAA,IAAA,KAAA,QAAA,CAAA,CAAA,GAAA,KAAA,cAAA,CAAA,MAAA,GAAA,MAAA,CAAA,MAAA,EAAA;AACA,aAAA,QAAA,CAAA,CAAA,IAAA,CAAA,CAAA;AACA,QAAA,YAAA,GAAA,IAAA;AACA;;AAEA,aAAA,YAAA;AACA;AA/CA,GA5BA;AA6EA,EAAA,OA7EA,qBA6EA,CAEA;AA/EA,CAAA","sourcesContent":["<template>\n    <div class=\"coming-soon\">\n        <vue-p5\n                @preload=\"preload\"\n                @setup=\"setup\"\n                @draw=\"draw\"\n        ></vue-p5>\n    </div>\n</template>\n\n<script>\n    import VueP5 from 'vue-p5';\n    import p5 from 'p5';\n    import logo from '../assets/logo_image.svg'\n\n\n    export default {\n        name: 'ComingSoon',\n        components: {\n            'vue-p5': VueP5\n        },\n        data() {\n            return {\n                position: null,\n                velocity: null,\n                image: null,\n                test: null,\n                colors: [\n                    '#9b5de5',\n                    '#f15bb5',\n                    '#fee440',\n                    '#00bbf9',\n                    '#00f5d4'\n                ],\n                collisionIndex: 0\n            }\n        },\n        computed: {\n            logoDimensions() {\n                return this.isMobile()\n                    ? {width: this.image.width/3, height: this.image.height/3}\n                    : {width: this.image.width, height: this.image.height}\n            }\n        },\n        methods: {\n            isMobile() {\n                try {document.createEvent(\"TouchEvent\");return true;}\n                catch(e){return false;}\n            },\n            preload(sketch) {\n                this.position = sketch.createVector(0,0);\n                this.velocity = new p5.Vector.fromAngle(45);\n                this.velocity.mult(5);\n                this.image = sketch.loadImage(logo);\n                sketch.frameRate(50);\n            },\n            setup(sketch) {\n                sketch.createCanvas(window.innerWidth, window.innerHeight);\n                sketch.tint(this.colors[this.collisionIndex])\n            },\n            draw(sketch) {\n                sketch.background('#111');\n\n                // Do something when image hits boundary\n                let hasCollision = this.checkBoundaryCollision(sketch)\n                if (hasCollision) {\n                    this.collisionIndex >= this.colors.length-1 ? this.collisionIndex = 0  : this.collisionIndex++\n                    sketch.tint(this.colors[this.collisionIndex])\n                }\n\n                this.position.add(this.velocity.x, this.velocity.y, this.velocity.z)\n\n                sketch.image(this.image, this.position.x, this.position.y, this.logoDimensions.width, this.logoDimensions.height)\n\n            },\n            checkBoundaryCollision(sketch) {\n                let hasCollision = false;\n\n                // left or right collision\n                if (this.position.x < 0 || this.position.x + this.logoDimensions.width > sketch.width) {\n                    this.velocity.x *= -1;\n                    hasCollision = true;\n                }\n\n                // top or bottom collision\n                if (this.position.y < 0 || this.position.y + this.logoDimensions.height > sketch.height)  {\n                    this.velocity.y *= -1;\n                    hasCollision = true;\n                }\n\n                return hasCollision\n            }\n        },\n        mounted() {\n\n        }\n    }\n</script>\n\n<style lang=\"scss\">\n    .coming-soon {\n        position: fixed;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n    }\n</style>\n"],"sourceRoot":"src/components"}]}